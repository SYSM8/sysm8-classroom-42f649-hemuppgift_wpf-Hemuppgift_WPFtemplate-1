<Window x:Class="Hemuppgift_WPFtemplate.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Hemuppgift_WPFtemplate"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>

        <Button Name="BtnGrid"  Content="Grid" FontSize="30" Click="BtnGrid_Click" Background="Transparent" />
        <Button Name="BtnStock" Content="StackPanel" FontSize="30" Click="BtnStock_Click" Background="Transparent" Grid.Column="1" Grid.Row="0"/>
        <Button Name="BtnWrap" Content="WrapPanel" FontSize="30" Click="BtnWrap_Click" Background="Transparent" Grid.Column="0" Grid.Row="1"/>
        <Button Name="BtnDock" Content="DockPanel" FontSize="30" Click="BtnDock_Click" Background="Transparent" Grid.Column="1" Grid.Row="1"/>






    </Grid>
</Window>
